{
  "name": "warping-management",
  "version": "0.8.0",
  "main": "main.js",
  "private": true,
  "productName": "Warping Inventory",
  "proxy": "http://localhost:7227",
  "build": {
    "extends": null,
    "appId": "com.yantra.warpingmanagement",
    "copyright": "Copyright Â© 2021 Yantra Soft",
    "nsis": {
      "oneClick": false,
      "perMachine": true
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "app.ico"
    },
    "files": [
      "*.js",
      "./build/**/*",
      "./db/**/*",
      "./routers/**/*",
      "./src/activate_utils.js"
    ]
  },
  "dependencies": {
    "@react-pdf/renderer": "^2.0.21",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.0",
    "get-port": "^5.1.1",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.0",
    "moment": "^2.29.1",
    "react-data-grid": "^7.0.0-beta.7",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.2",
    "systeminformation": "^5.7.7",
    "umzug": "^2.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:electron": "yarn run electron .",
    "pack": "electron-builder --win",
    "dist": "yarn run build && yarn run pack",
    "rebuild": "yarn run electron-rebuild -f -w",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "date-fns": "^2.21.1",
    "electron": "^16.0.1",
    "electron-builder": "^22.13.1",
    "electron-rebuild": "^2.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.0",
    "react-table": "^7.6.3",
    "react-to-print": "^2.12.4",
    "redux": "^4.1.0",
    "simple-react-validator": "^1.6.0",
    "tslint": "^6.1.3"
  }
}
